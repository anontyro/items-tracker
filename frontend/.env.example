# Frontend (Next.js) example environment
# Copy to .env.local and adjust values for local development

# Internal URL for the NestJS backend API used by Next.js route handlers
BACKEND_API_URL=http://localhost:3005

# API key that Next.js uses when calling the backend (sent as x-api-key)
FRONTEND_API_KEY=change-me-frontend

# Admin API key used by the browser to call admin-only frontend routes
ADMIN_API_KEY=change-me-admin
